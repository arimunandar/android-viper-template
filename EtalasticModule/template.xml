<template format="4"
		revision="1"
		name="Etalastic Module"
		description="Creates a new Etalastic classes">
 
	<category value="Other"/>
 
	<parameter id="className"
		name="Functionality Name"
		type="string"
		constraints="class|unique|nonempty"
		default="Etalastic Backend Module Name"
		help="The name of the functionality that requires Etalastic views"/>
 
	<globals file="globals.xml.ftl" />
    <execute file="recipe.xml.ftl" />
 
</template>